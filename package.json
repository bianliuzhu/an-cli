{
	"name": "anl",
	"version": "26.116.0",
	"description": "FE command line tool",
	"main": "bin/an-cli.js",
	"publishConfig": {
		"registry": "https://registry.npmjs.org"
	},
	"scripts": {
		"dev": "tsdown --watch",
		"build": "rimraf lib && tsdown",
		"pub": "bash publish.sh",
		"ts": "tsc ./src/int.ts --noEmit --watch",
		"blink": "npm run build && npm link",
		"sync-docs": "node scripts/sync-docs.js",
		"docs:check": "node scripts/docs-manager.js check",
		"docs:copy": "node scripts/docs-manager.js copy",
		"docs:sync": "node scripts/docs-manager.js sync",
		"docs:validate": "node scripts/docs-manager.js validate",
		"docs:stats": "node scripts/docs-manager.js stats",
		"docs:setup": "npm run docs:copy && npm run docs:sync",
		"docs:serve": "cd docs && (python3 -m http.server 3000 || npx http-server -p 3000)"
	},
	"bin": {
		"anl": "bin/an-cli.js"
	},
	"author": "Gleason <bianliuzhu@gmail.com>",
	"license": "ISC",
	"devDependencies": {
		"@commitlint/cli": "^17.4.3",
		"@commitlint/config-conventional": "^17.4.3",
		"@types/inquirer": "^9.0.7",
		"@types/shelljs": "^0.8.11",
		"@typescript-eslint/eslint-plugin": "^5.52.0",
		"@typescript-eslint/parser": "^5.52.0",
		"cross-env": "^7.0.3",
		"eslint": "^8.7.0",
		"husky": "^8.0.3",
		"openapi-types": "^12.1.3",
		"prettier": "^3.3.2",
		"rimraf": "^5.0.7",
		"tsdown": "^0.20.0-beta.3",
		"typescript": "^5.9.3"
	},
	"dependencies": {
		"app-root-path": "^3.1.0",
		"cac": "^6.7.12",
		"chalk": "4.*",
		"clear-console": "^1.1.0",
		"commander": "14.0.1",
		"figures": "^6.1.0",
		"inquirer": "^10.1.8",
		"log-symbols": "^5.1.0",
		"ora": "5.*",
		"progress-estimator": "^0.3.0",
		"shelljs": "^0.8.5"
	},
	"keywords": [
		"cli",
		"command-line",
		"frontend",
		"typescript",
		"type-generation",
		"code-generator",
		"swagger",
		"openapi",
		"api",
		"rest-api",
		"eslint",
		"stylelint",
		"prettier",
		"commitlint",
		"linter",
		"formatter",
		"react",
		"vue",
		"git",
		"gitflow",
		"developer-tools",
		"automation",
		"scaffolding"
	],
	"files": [
		"package.json",
		"README.md",
		"lib",
		"template"
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/bianliuzhu/an-cli.git"
	},
	"type": "commonjs"
}
